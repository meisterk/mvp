<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/8.0.191" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="textFieldInput"/>
      <Button fx:id="button" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Do something" />
      <Label fx:id="labelOutput" alignment="CENTER" maxWidth="1.7976931348623157E308" text="Output" textAlignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
   </children>
</VBox>
